sig.drop1<-as.numeric(summary(drop1)$sigma^2)
sig.drop2<-as.numeric(summary(drop2)$sigma^2)
sig.rep1<-as.numeric(summary(rep1)$sigma^2)
sig.rep2<-as.numeric(summary(rep2)$sigma^2)
