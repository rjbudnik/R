sig.Repless1<-as.numeric(summary(Repless1)$sigma^2)
sig.Repless2<-as.numeric(summary(Repless2)$sigma^2)
sig.linear1<-as.numeric(summary(Linear1)$sigma^2)
sig.linear2<-as.numeric(summary(Linear2)$sigma^2)
