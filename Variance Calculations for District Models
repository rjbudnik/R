  #Error DF
  error_df<- as.data.frame(summary1$df.residual)
  error_df1 <- error_df %>%
  #rename variable
  select (Df = starts_with("summary"), everything())
  #MS of error
  mse1 <- as.data.frame(error_ss1/error_df1)
